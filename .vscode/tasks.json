{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "go-build",
            "type": "shell",
            "command": "docker exec -it go_build_ping-grpc-client target_data/ping-grpc-client/.script/go_build.sh 'linux' 'amd64' 'build/ping-grpc-client'",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$go"
            ]
        },
        {
            "label": "go-build-windows",
            "type": "shell",
            "command": "docker exec -it go_build_ping-grpc-client target_data/ping-grpc-client/.script/go_build.sh 'windows' 'amd64' 'build/win-ping-grpc-client.exe'",
            "args": [],
            "problemMatcher": [
                "$go"
            ]
        },
        {
            "label": "go-build-release",
            "type": "shell",
            "command": "docker exec -it go_build_ping-grpc-client target_data/ping-grpc-client/.script/go_build_release.sh",
            "args": [],
            "problemMatcher": [
                "$go"
            ]
        },
        {
            "label": "develop container build",
            "type": "shell",
            "command": "_USER=\"$(id -u):$(id -g)\" docker-compose -f .docker/docker-compose.yml build",
            "problemMatcher": []
        },
        {
            "label": "develop container start",
            "type": "shell",
            "command": "_USER=\"$(id -u):$(id -g)\" docker-compose -f .docker/docker-compose.yml up",
            "problemMatcher": []
        },
        {
            "label": "develop container stop",
            "type": "shell",
            "command": "_USER=\"$(id -u):$(id -g)\" docker-compose -f .docker/docker-compose.yml stop",
            "problemMatcher": []
        },
        {
            "label": "develop container remove",
            "type": "shell",
            "command": "_USER=\"$(id -u):$(id -g)\" docker-compose -f .docker/docker-compose.yml down",
            "problemMatcher": []
        },
        {
            "label": "protobuf build in container",
            "type": "shell",
            "command": "docker exec -it proto_build_ping-grpc-client target_data/.script/proto_build.sh",
            "problemMatcher": []
        }
    ]
}